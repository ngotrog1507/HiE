    <script>
        $(function () {
            loadNotification();
        })
        function loadNotification() {
            $.ajax({
                type: "POST",
                url: '@Url.Action("GetNotificationQuestion", "Common")',
                contentType: "application/json; charset=utf-8",
                data: "{}",
                dataType: "json",
                success: function (res) {
                    if (res.status === "ok") {
                        bindNotification(res.value)
                    }
                }, error: function (msg) {
                    console.log("Lỗi tải Các Thoong baso");
                }
            });
        }
        function bindNotification(item) {
            console.log(item);
            var totalNew = 0;
            var html = "";
            for (var i = 0; i <item.length; i++) {
                    html += "<li>";
                    html += "       <a href='#' style='padding: 8px 0px 0px 11px; '>";
                    html += "           <div class='pull-left' style='padding-right: 14px;'>";
                    html += "               <img src='/user.png' class='img-circle' style='width:63px;height:63px' alt='User Image'>";
                    html += "           </div>";
                    html += "           <h4 style=' font-size: 16px;'>" + item[i].FullName + "</h4>";
                    html += "               <small><i class='fa fa-clock-o'></i> " + item[i].CreatedDateStr + "</small>    ";
                    html += "           <p style=' font-size: 13px;'>" + item[i].Comments +"</p>";
                    html += "       </a>";
                html += " </li>";
                if (!item[i].IsActive) {
                    totalNew += 1;
                }
            }
            $('#notifications').prepend(html);
            $(".totalNoti").text(parseInt($(".totalNoti").text()) + totalNew);
        }
    </script>
