            constructor(obj, html) {
                this.obj = obj;
                this.html = $("#" + obj).html();
            }
            start() {
                $("#" + this.obj).attr("disabled", "disabled");
                $("#" + this.obj).html("<i class='fa fa-cog fa-spin' aria-hidden='true'></i>&nbsp; Đang xử lý &nbsp;");
